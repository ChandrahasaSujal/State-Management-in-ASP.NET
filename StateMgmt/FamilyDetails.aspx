<%@ Page Language="C#" AutoEventWireup="true" CodeBehind="FamilyDetails.aspx.cs" Inherits="StateMgmt.Family_Details" %>

<!DOCTYPE html>

<html xmlns="http://www.w3.org/1999/xhtml">
<head runat="server">
    <title></title>
</head>
<body>
